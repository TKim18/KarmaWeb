<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Provider</th>
      <th>Uid</th>
      <th>Name</th>
      <th>Created At</th>
      <th>Email</th>
      <th>fullName</th>
      <th>karmaPoints</th>
      <th>circleName</th>
      <th>homeLocation</th>
      <th>circleId</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.provider %></td>
        <td><%= user.uid %></td>
        <td><%= user.name %></td>
        <td><%= user.created_at %></td>
        <td><%= user.email %></td>
        <td><%= user.fullName %></td>
        <td><%= user.karmaPoints %></td>
        <td><%= user.circleName %></td>
        <td><%= user.homeLocation %></td>
        <td><%= user.circle_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
